@model AirplaneOwner[]
@{
    ViewData["Title"] = "Home Page";
}
<table>
    <tr>
        <th>ID</th>
        <th>Название</th>
        <th>Дата Рождения</th>
        <th>Уровень пилотирования</th>
        <th>Количество самолетов</th>
        <th>Место жительства</th>
        <th>Действие</th>
    </tr>
    @for (int i = 0; i < Model.Length; i++)
    {
<tr>
    <th>@Model[i].AeroClubId</th>
    <th>@Model[i].Name</th>
    <th>@Model[i].CreationDate</th>
    <th>@Model[i].Type</th>
    <th>@Model[i].NumberOfPlanes</th>
    <th>@Model[i].Location</th>
    <th>
        <a class="btn btn-primary" href="@Url.Action("EditAirplaneOwner", new {id = i})">Изменить</a>
        <a class="btn btn-primary" href="@Url.Action("GetAirplaneOwner", new {id = i})">Посмотреть</a>
    </th>
</tr>
    }
</table>
<a class="btn btn-primary" asp-action="AddAirplaneOwner">Добавить</a>

